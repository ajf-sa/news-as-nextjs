{
    "compilerOptions": {
        "baseUrl": ".",
        "sourceMap": true,
        "outDir": "dist",
        "strict": true,
        "lib": [
            "ESNext",
            "DOM"
        ],
        "esModuleInterop": true,
        "target": "es5",
        "allowJs": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "module": "ESNext",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve"
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx"
    ],
    "exclude": [
        "node_modules"
    ]
}